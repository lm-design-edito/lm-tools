{
  "compilerOptions": {
    "lib": ["ESNext"],
    "strict": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "../lib",
    "declaration": true,
    "moduleResolution": "Node",
    "rootDir": ".",
    "noUncheckedIndexedAccess": true,
    "jsx": "react-jsx"
  },
  "exclude": [
    "../node_modules",
    "../lib"
  ]
}
